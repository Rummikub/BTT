    "# fit the model on the training data\n",
    "# check time\n",
    "# start time\n",
    "st = time.time()\n",
    "num_epochs = 50\n",
    "history = nn_model.fit(x=X_train_tfidf.toarray(), y=y_train,epochs=num_epochs, verbose=0, callbacks=[ProgBarLoggerNEpochs(num_epochs, every_n=5)],\n",
    "                       validation_split=.2)\n",
    "# end time\n",
    "et = time.time()\n",
    "print('Total time: %.2fs' % (et-st))"
