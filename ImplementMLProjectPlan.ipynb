   "outputs": [],
   "source": [
    "# Create Labeled Examples\n",
    "y=df['Positive Review']\n",
    "X=df['Review']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "data": {
